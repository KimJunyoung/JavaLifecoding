package basic_section5_1;

public class BestCustomer extends Customer{

	
	private int agentID;
	double salesRatio;
	
	public BestCustomer(int customer) {
		customerGrade = "VIP";
		salesRatio = 0.05;
		bonusRatio = 0.1;
		System.out.println(customer);
	}
	
	public int getAgentId() {
		return agentID;
	}
}
