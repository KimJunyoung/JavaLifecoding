package basic_section3_3;

public class Person {

	private String name ;
	private int age;
	
	public Person() {
		this("이름없다",1);
		this.age = 2;
	};
	
	public Person(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
	public Person getPerson() {
		return this;
	}
	
	public static void main(String[] args) {
		Person person = new Person();
		System.out.println(person.name);
		System.out.println(person.age);
	}
}
